    <Header>
        <ID>clarin.eu:cr1:p_1659015263825</ID>
        <Name>BiodatenMinimal</Name>
        <Description>Please describe your data package with scientific metadata. This form offers you the possibility to describe the studied objects of your data package, the methods you used, and your data files.</Description>
        <Status>development</Status>
        <DerivedFrom>clarin.eu:cr1:p_1659015263808</DerivedFrom>
    </Header>
    <Component name="BiodatenMinimal" CardinalityMin="1" CardinalityMax="1">
        <Component name="StudiedObject" ComponentRef="clarin.eu:cr1:c_1659015263811" ConceptLink="https://bioschemas.org/types/Sample" CardinalityMin="1" CardinalityMax="unbounded">
            <Documentation>Sample: a material entity that is representative of a whole, see also http://purl.jp/bio/4/id/200906034237481745</Documentation>
            <Element name="studiedObjectIdentifier" ConceptLink="https://schema.org/identifier" ValueScheme="string" CardinalityMin="1" CardinalityMax="1" cue:dependsOn_autogeneration="true" xmlns:cue="http://www.clarin.eu/cmd/cues/1">
                <Documentation>unique identifier of the studied object (autogenerated)</Documentation>
            </Element>
            <Element name="descriptionStudiedObject" ValueScheme="string" CardinalityMin="1" CardinalityMax="1" cue:display_placeholder="add free text (e.g., Arabidopsis thaliana)" xmlns:cue="http://www.clarin.eu/cmd/cues/1">
                <Documentation>description of studied object</Documentation>
            </Element>
            <Element name="typeStudiedObject" CardinalityMin="1" CardinalityMax="1" cue:dependsOn_valueSelection="'Organism (i.e., eukaryota, bacteria, archaea)'--organism;'Organism (i.e., eukaryota, bacteria, archaea)'--cellType" xmlns:cue="http://www.clarin.eu/cmd/cues/1">
                <Documentation>name type of studied object</Documentation>
                <ValueScheme>
                    <Vocabulary>
                        <enumeration>
                            <item>Organism (i.e., eukaryota, bacteria, archaea)</item>
                            <item>Non-organism (e.g., virus, water, environmental RNA)</item>
                        </enumeration>
                    </Vocabulary>
                </ValueScheme>
            </Element>
            <Element name="organism" ConceptLink="http://purl.jp/bio/4/id/200906079942882108" ValueScheme="anyURI" CardinalityMin="0" CardinalityMax="1" cue:DisplayPriority="1" xmlns:cue="http://www.clarin.eu/cmd/cues/1">
                <Documentation>name of the organism used in the sample; use a controlled vocabulary by make reference to an ontology such as http://purl.obolibrary.org/obo/OBI_0100026</Documentation>
            </Element>
            <Element name="cellType" ConceptLink="http://purl.jp/bio/4/id/201106077286713294" ValueScheme="anyURI" CardinalityMin="0" CardinalityMax="1" cue:DisplayPriority="1" xmlns:cue="http://www.clarin.eu/cmd/cues/1">
                <Documentation>name of the cell type used in the sample. Use controlled vocabulary such as https://bioportal.bioontology.org/ontologies/CL or http://purl.obolibrary.org/obo/CL_0000000</Documentation>
            </Element>
            <Element name="additionalInformation" ValueScheme="string" CardinalityMin="1" CardinalityMax="1" cue:display_placeholder="add free text (e.g., arabidopsis seedling, leaf, root etc.)" xmlns:cue="http://www.clarin.eu/cmd/cues/1">
                <Documentation>Please provide additional information</Documentation>
            </Element>
            <Element name="nameMaterial" ConceptLink="http://purl.jp/bio/4/id/200906048950987544" ValueScheme="anyURI" CardinalityMin="0" CardinalityMax="1" cue:DisplayPriority="1" xmlns:cue="http://www.clarin.eu/cmd/cues/1">
                <Documentation>name of the material (structure, substance, device) removed from a source (patient, donor, physical location, product) OR a material entity that has the specimen role. Please use controlled vocabulary  such as http://purl.obolibrary.org/obo/OBI_0100051</Documentation>
            </Element>
            <Element name="measurementTarget" ValueScheme="anyURI" CardinalityMin="0" CardinalityMax="1" cue:DisplayPriority="1" xmlns:cue="http://www.clarin.eu/cmd/cues/1">
                <Documentation>measurement target (e.g., protein, gene, compound)</Documentation>
            </Element>
